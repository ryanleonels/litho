[maven_repositories]
  jcenter = https://jcenter.bintray.com/
  nuiton = https://nexus.nuiton.org/nexus/content/groups/releases/
  central = https://repo1.maven.org/maven2/
  google = https://maven.google.com/

[ndk]
  ndk_version = 15.2.4203891
  compiler = clang
  app_platform = android-30
  cpu_abis = arm64, armv7, x86, x86_64

[android]
  target = android-30
  build_tools_version = 30.0.2

[tools]
  java11_for_tests = "/usr/local/java-runtime/impl/11/bin/java"

[cxx]
  link_path_normalization_args_enabled = true
